<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--数据库信息-->
  <Database type="Iwenli.Data.DatabaseConfig">
    <ConnString>
    </ConnString>
  </Database>
  <!--日志调试信息-->
  <log4net path="./_log/">
    <!--默认日志级别，OFF、Fatal、ERROR、WARN、INFO、DEBUG、ALL-->
    <root>
      <level value="ALL" />
    </root>
    <!--日志记录对象-->
    <logger name="wl.Config">
      <level value="ALL" />
      <appender-ref ref="Iwenli.Config.Appender" />
    </logger>
  </log4net>
</configuration>