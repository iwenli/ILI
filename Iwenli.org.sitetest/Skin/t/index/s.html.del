<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>买客</title>
    <!--WL_TMP{tmp=/Skin/t/Pub/app_publiccss.html}-->
    <link href="/Skin/t/Css/sui/index.css?v=4.1" rel="stylesheet" />
</head>
<body>
    <div class="page-group">
        <!--搜索-->
        <div id="page-index-search" class="page">
            <header class="bar bar-nav search-title" style="padding-right:0;">
                <a class="icon icon-left pull-left back" style="width:8%;box-sizing:border-box;"></a>
                <div class="searchbar" style="width:92%;box-sizing:border-box;">
                    <a class="searchbar-cancel j-search">搜索</a>
                    <div class="search-input">
                        <form id="product-search-form" action="" onsubmit="return false;">
                            <label class="icon icon-search" for="search" style="color: #524d4d;"></label>
                            <input type="search" id="product-search-input" class="j-sk" onkeyup="value = value.replace(/[#,*]/g, '');"
                                   placeholder='输入关键字...' autofocus autocomplete="off"
                                   style="padding-left: 1.5rem;" />
                        </form>
                    </div>
                </div>
            </header>
            <div class="content search_fff" style="background:#fff;">
                <div class="module-tempalte">
                    <div class="tempalte-title">
                        <div style="margin-bottom:0.5rem;">
                            <span class="pull-left" style="color:#aaa;font-size:0.75rem;">搜索历史</span>
                            <span class="icon icon-remove pull-right j-removeAll" style="color:#aaa;font-size:0.75rem;"></span>
                            <div style="clear:both"></div>
                        </div>
                        <div class="keyTag-container j-keytag-con">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--搜索结果-->
        <div id="page-index-result" class="page">
            <header class="bar bar-nav search-title">
                <a class="icon icon-left pull-left j-back-index" href="#" style="margin-left:0.5rem;color:#FFF"></a>
                <div class="searchbar row" style="margin:0;">
                    <div class="search-input col-90">
                        <input type="search" class="j-sk" readonly="readonly" />
                    </div>
                    <a href="#" title="消息" class="msg col-10  j-app-active"><i>&#xe676;</i></a>
                </div>
                <div class="buttons-tab">
                    <a href="#s-content1" data-type="1" class="tab-link button active">综合排序</a>
                    <a href="#s-content2" data-type="2" class="tab-link button">销量优先</a>
                    <a href="#s-content3" data-type="3" class="tab-link button">新品优先</a>
                    <a href="#s-content4" data-type="4" class="tab-link button price desc">价格<em></em></a>
                </div>
            </header>
            <div class="content infinite-scroll infinite-scroll-bottom">
                <div class="j-search-con">
                    <div class="tabs">
                        <div id="s-content1" class="tab active">
                            <div class="module-tempalte">
                            </div>
                            <!-- 加载提示符 -->
                            <div class="infinite-scroll-preloader">
                                <div class="preloader">
                                </div>
                            </div>
                        </div>
                        <div id="s-content2" class="tab">
                            <div class="module-tempalte">
                            </div>
                            <!-- 加载提示符 -->
                            <div class="infinite-scroll-preloader">
                                <div class="preloader">
                                </div>
                            </div>
                        </div>
                        <div id="s-content3" class="tab">
                            <div class="module-tempalte">
                            </div>
                            <!-- 加载提示符 -->
                            <div class="infinite-scroll-preloader">
                                <div class="preloader">
                                </div>
                            </div>
                        </div>
                        <div id="s-content4" class="tab">
                            <div class="module-tempalte">
                            </div>
                            <!-- 加载提示符 -->
                            <div class="infinite-scroll-preloader">
                                <div class="preloader">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--WL_TMP{tmp=/Skin/t/index/temp/index_template.html}-->
    </div>
    <!--WL_TMP{tmp=/Skin/t/Pub/app_publicjs.html}-->
    <script src="/Skin/t/Js/sui/index.js?v=4"></script>
</body>
</html>